{
    "name": "grosv/laravel-cloudinary",
    "description": "A repository template for Laravel packages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ed Grosvenor",
            "email": "ed.grosvenor@wickedviral.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "illuminate/support": "^6.0|^7.0",
        "cloudinary/cloudinary_php": "^1.16"
    },
    "require-dev": {
        "orchestra/testbench": "^4.6"
    },
    "autoload": {
        "files": [
            "scripts/helpers.php"
        ],
        "psr-4": {
            "Grosv\\LaravelCloudinary\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "vendor/orchestra/testbench-core/laravel/app"
        }
    },
    "extra": {
        "laravel": {
            "providers": "Grosv\\LaravelCloudinary\\LaravelCloudinaryProvider"
        }
    }
}
